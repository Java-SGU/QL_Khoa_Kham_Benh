    //dua du lieu vao bang
    private void LayDLVaoBang(){
         try {
            modelNgDung = new DefaultTableModel();
            String muc[] = {"ID", "Tên đăng nhập", "Mã nhân viên", "Email"};
            modelNgDung.setColumnIdentifiers(muc);
            for(DTONguoiDung dtoNgDung : bllNgDung.LayDSNguoiDung()){
                modelNgDung.addRow(new Object[]{dtoNgDung.getiD(), dtoNgDung.getTenDN(), 
                    dtoNgDung.getDtoNV().getMaNV(), dtoNgDung.getEmail()});
            }
            jTbDSNguoiDung.setModel(modelNgDung);
        } catch (Exception e) {
            System.err.print("Lấy dữ liệu không thành công");
        }
    }

