    private void jTbDSNguoiDungMousePressed(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jTbDSNguoiDungMousePressed
        try {
            int maNgDung = Integer.valueOf(jTbDSNguoiDung.getValueAt(jTbDSNguoiDung.getSelectedRow(), 0).toString());
            DTONguoiDung dtoNgDung = bllNgDung.layNguoiDungTheoMa(maNgDung);
            if(dtoNgDung != null){
                jtxtID.setText(String.valueOf(dtoNgDung.getiD()));
                jtxtDangNhap.setText(dtoNgDung.getTenDN());
                jtxtEmail.setText(dtoNgDung.getEmail());
                jtxtMaNV.setText(String.valueOf(dtoNgDung.getDtoNV().getMaNV()));
                jtxtHoTen.setText(dtoNgDung.getDtoNV().getHoTen());
                //giới tính
                jtxtNgaySinh.setText(String.valueOf(dtoNgDung.getDtoNV().getNgaySinh()));
                jtxtCmnd.setText(dtoNgDung.getDtoNV().getCmnd());
                jtxtDienThoai.setText(dtoNgDung.getDtoNV().getDienThoai());
                jtxtDiaChi.setText(dtoNgDung.getDtoNV().getDiaChi());
                jtxtChuyenKhoa.setText(dtoNgDung.getDtoNV().getDtoCK().getTenChuyenKhoa());
                //học vị: bác sĩ/ y tá
                jtxtChucVu.setText(dtoNgDung.getDtoNV().getDtoCV().getTenChucVu());
            }
            else
                JOptionPane.showMessageDialog(null, "Khong co nguoi dung nay");
        } catch (Exception e) {
        }
    }//GEN-LAST:event_jTbDSNguoiDungMousePressed

